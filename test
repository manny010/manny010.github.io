<button onclick="stuff()">Chlick</button>
<script>
function stuff() {
    var user = prompt("Choose a # from 0-10");
    var randm = Math.floor(Math.random() * 11);
    console.log(randm)
    if (user == randm) {
        console.log("you got it right")
    }
    else {
        console.log("you got it wrong!!!! haha")
    }
}
</script>
